
<h2>履歴一覧</h2>

<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.tm_table.revision_number') %></th>
      <th><%= t('activerecord.attributes.tm_table.note') %></th>
    </tr>
  </thead>

  <tbody>
    <% @tm_table.tm_table_histories.each do |tm_table| %>
      <tr>
        <td><%= tm_table.revision_number %></td>
        <td><%= tm_table.note %></td>
      </tr>
    <% end %>
  </tbody>
</table>
